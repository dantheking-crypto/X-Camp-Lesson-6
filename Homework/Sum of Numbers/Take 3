#include <iostream>
using namespace std;
int main() {
  int numbers, queries;
  cin >> numbers;
  int Numbers[numbers];
  for (int i = 0; i < numbers; i++) {
    cin >> Numbers[i];
  }
  cin >> queries;
  int results[queries];
  for (int i = 0; i < queries; i++) {
     int l, r;
     cin >> l >> r;
     int sum = 0;
     for (int i = l - 1; i < r; i++) {
       sum += Numbers[i];
     }
     results[i] = sum;
  }
  for (int i = 0; i < queries; i++) {
    cout << results[i] << endl;
  }
}
