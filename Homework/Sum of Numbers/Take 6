#include <iostream>
using namespace std;
int main() {
  int numOfNum, queries;
  cin >> numOfNum;
  int Numbers[numOfNum];
  for (int i = 0; i < numOfNum; i++) {
    cin >> Numbers[i];
  }
  cin >> queries;
  int results[queries];
  for (int i = 0; i < queries; i++) {
     int l, r;
     cin >> l >> r;
     if (l < 1) l = 1;
     if (r > numOfNum) r = numOfNum;
     int sum = 0;
     for (int j = l - 1; j < r; j++) {
       sum += Numbers[j];
     }
     results[i] = sum;
  }
  for (int i = 0; i < queries; i++) {
    cout << results[i] << endl;
  }
  return 0;
}
